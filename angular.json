{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ces-society": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ces-society",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.png",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/animate.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/slick.css",
              "src/assets/css/off-canvas.css",
              "src/assets/fonts/linea-fonts.css",
              "src/assets/fonts/flaticon.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/rsmenu-main.css",
              "src/assets/css/rs-spacing.css",
              "src/assets/css/style.css",
              "src/assets/css/responsive.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": [
              "src/assets/js/modernizr-2.8.3.min.js",
              "src/assets/js/jquery.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/rsmenu-main.js",
              "src/assets/js/jquery.nav.js",
              "src/assets/js/wow.min.js",
              "src/assets/js/plugins.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/contact.form.js",
              "src/assets/js/main.js"

            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kb",
                  "maximumError": "8kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ces-society:build:production"
            },
            "development": {
              "browserTarget": "ces-society:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ces-society:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.png",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/animate.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/slick.css",
              "src/assets/css/off-canvas.css",
              "src/assets/fonts/linea-fonts.css",
              "src/assets/fonts/flaticon.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/rsmenu-main.css",
              "src/assets/css/rs-spacing.css",
              "src/assets/css/style.css",
              "src/assets/css/responsive.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css"

            ],
            "scripts": [
              "src/assets/js/modernizr-2.8.3.min.js",
              "src/assets/js/jquery.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/rsmenu-main.js",
              "src/assets/js/jquery.nav.js",
              "src/assets/js/wow.min.js",
              "src/assets/js/plugins.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/contact.form.js",
              "src/assets/js/main.js"
              
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ces-society"
}
